    <rdfs:label xml:lang="ar">ألبرت (الأمير الزوج)</rdfs:label>
    <rdfs:label xml:lang="es">Alberto de Sajonia-Coburgo-Gotha</rdfs:label>
    <rdfs:label xml:lang="pt">Alberto de Saxe-Coburgo-Gota</rdfs:label>
